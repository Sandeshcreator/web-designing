{
  "name": "webdesigning",
  "version": "1.0.0",
  "scripts": {
    "tw:build": "tailwindcss -i ./src/input.css -o ./src/output.css --content \"./src/**/*.html\"",
    "tw:watch": "tailwindcss -i ./src/input.css -o ./src/output.css --watch --content \"./src/**/*.html\""
  },
  "devDependencies": {
    "tailwindcss": "^4.1.11",
    "@tailwindcss/cli": "^4.1.11"
  }
}
